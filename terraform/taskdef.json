[
  {
    "name": "streetmerchant",
    "image": "ghcr.io/jef/streetmerchant:latest",
    "cpu": ${cpu},
    "memory": ${memory},
    "essential": true,
    "environment": [
      {
        "name": "SHOW_ONLY_SERIES",
        "value": "sonyps5c,sonyps5de"
      },
      {
        "name": "SLACK_CHANNEL",
        "value": "ps5"
      },
      {
        "name": "SLACK_TOKEN",
        "value": "xoxb-281413785376-1660072221158-L0nUj8g7o0sf0VgZARfngull"
      },
      {
        "name": "STORES",
        "value": "amazon-uk,game,argos,box,currys,johnlewis,shopto,smythstoys,very"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs-group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
